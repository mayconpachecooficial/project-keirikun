                    <h4 id="sechedule1-monday-time" class="sechedule-time" style='background-color: white'>
                        <%= days[i].START_TIME%>~<%= days[i].FINISH_TIME%>
                    </h4>
                  <% } else if(days[i].COLOR == 'purple'){ %>  
                    <h4 id="sechedule1-monday-time" class="sechedule-time" style='background-color: purple'>
                        <%= days[i].START_TIME%>~<%= days[i].FINISH_TIME%>
                    </h4>
                 <% } %>

              <div id="sechedule1-monday-div" class="sechedule-div">
                <div class="title-image-div">
                <h1 id="sechedule1-monday-title1" class="sechedulename"><%= days[i].DESCRITION_1%></h1>
                <h1 id="sechedule1-monday-title2" class="sechedulename"><%= days[i].DESCRITION_2%></h1>
                </div>
                <% if(days[i].IMAGE == 'monday1'){ %>
                    <%- include('./partials/kimonoTag1')%>
                <% } else if(days[i].IMAGE == 'monday2'){ %>  
                    <%- include('./partials/kimonoTag2')%>
                <% }%>
              </div>
            </div>
          <%}%>
      </div>
    </div>
  </body>
</html>

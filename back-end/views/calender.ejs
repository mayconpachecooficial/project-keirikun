    <style>
      html {
        overflow-y: hidden;
        height: 100%;
      }

      body {
        height: 100%;
        margin: 0;
      }
      #sotowaku {
        height: 100%;
      }
      #titlediv {
        height: 5%;
        background-color: #222222;
        display: -webkit-flex;
        -webkit-align-items: center;
        align-items: center;
        padding-right: 300px;
      }
      #gym-name {
        color: white;
        position: absolute;
        font-size: 3vh;
        right: 0;
      }
      #titlemain {
        text-align: left;
        margin-left: 0;
        font-size: 2vh;
        color: lightgray;
      }

      #maindiv {
        height: 90%;
        background-color: darkgray;
      }
      #settings-row {
        width: 100%;
        height: 7%;
      }
      #first-row {
        width: 100%;
        height: 5%;
        display: -webkit-flex;
        margin-top: 5px;
      }
      .sechedule-day {
        width: 14%;
        height: 90%;
        background-color: #666666;
        margin-left: 10px;
        margin-right: 10px;
        text-align: center;
        color: white;
        font-size: 3vh;
        vertical-align: middle;
      }
      .sechedule {
        width: 14%;
        height: 80%;
        background-color: #666666;
        margin-left: 10px;
        margin-right: 10px;
        text-align: center;
        color: white;
        font-size: 3vh;
        vertical-align: middle;
      }
      #second-row {
        height: 14%;
        display: -webkit-flex;
      }
      #third-row {
        height: 14%;
        display: -webkit-flex;
      }
      #fourth-row {
        height: 14%;
        display: -webkit-flex;
      }
      #fifth-row {
        height: 14%;
        display: -webkit-flex;
      }
      #sixth-row {
        height: 14%;
        display: -webkit-flex;
      }
      #settings-row {
        display: -webkit-flex;

        -webkit-justify-content: flex-end;
        justify-content: flex-end;
        margin-right: 10px;
      }
      #calendar-title {
        padding-right: 68%;
        font-size: 5vh;
      }
      #settings-button {
        height: 100%;
        padding-right: 30px;
      }
      .setting-right-button:hover {
        box-shadow: 0 5px 10px white;
      }

      #img {
        margin-right: 20px;
        cursor: pointer;
      }

      .sweet-alert-acess {
        height: 300px;
      }

      #update-time {
        background-color: orange;
      }

      #img2 {
